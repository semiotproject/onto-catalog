@prefix :      <http://base/#> .
@prefix tdb:   <http://jena.hpl.hp.com/2008/tdb#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ja:    <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fuseki: <http://jena.apache.org/fuseki#> .

#:service_tdb_all  a                   fuseki:Service ;
#        rdfs:label                    "TDB wot_semdesc_helper" ;
#        fuseki:dataset                :tdb_dataset_readwrite ;
#        fuseki:name                   "wot_semdesc_helper" ;
#        fuseki:serviceQuery           "query" , "sparql" ;
#        fuseki:serviceReadGraphStore  "get" ;
#        fuseki:serviceReadWriteGraphStore
#                "data" ;
#        fuseki:serviceUpdate          "update" ;
#        fuseki:serviceUpload          "upload" .
#
#:tdb_dataset_readwrite
#        a             tdb:DatasetTDB ;
#        tdb:location  "/etc/fuseki/databases/wot_semdesc_helper" .

tdb:GraphTDB rdfs:subClassOf ja:Model .

[] a fuseki:Server ;
   fuseki:services ( <#service1> ) ;
   .

<#service1> a fuseki:Service ;
	    fuseki:name 	 "ds" ;
	    fuseki:serviceQuery  "sparql" ;
		fuseki:serviceReadWriteGraphStore "data" ;
	    fuseki:serviceUpdate "update" ;
	    fuseki:dataset	 <#dataset> ;
	    .

<#dataset> a ja:RDFDataset ;
	   ja:defaultGraph <#model_inf> ;
           .

<#tdbDataset> a tdb:DatasetTDB ;
	   tdb:location "fuseki-db" ;
	   tdb:unionDefaultGraph true;
	   ja:defaultGraph <#model_inf>
	   .

<#model_inf> a ja:InfModel ;
	     ja:baseModel [
		a tdb:GraphTDB ;
		tdb:dataset <#tdbDataset> ;
	     ] ;
	     ja:content [
			ja:externalContent <http://purl.oclc.org/NET/ssnx/ssn#> ;
			#ja:externalContent <http://data.qudt.org/qudt/owl/1.0.0/qudt.owl> ;
			ja:externalContent <http://www.w3.org/ns/prov#> ;
			ja:externalContent <http://purl.org/NET/ssnext/electricmeters#> ;
			ja:externalContent <http://purl.org/NET/ssnext/heatmeters#> ;
			ja:externalContent <http://www.loa.istc.cnr.it/ontologies/DUL.owl> ;
			ja:externalContent <http://purl.org/NET/ssnext/machinetools#> ;
			ja:externalContent <http://purl.org/NET/ssnext/communication> ;
			ja:externalContent <http://qudt.org/1.1/vocab/unit#> ;
			ja:externalContent <http://qudt.org/1.1/vocab/quantity#> ;
			ja:externalContent <http://xmlns.com/foaf/0.1/> ;
			ja:externalContent <http://purl.org/ifgi/om> ;
			ja:externalContent <http://ontology.tno.nl/saref.ttl> ;
			ja:externalContent <http://www.w3.org/1999/02/22-rdf-syntax-ns> ;
			ja:externalContent <http://www.w3.org/2000/01/rdf-schema> ;
			ja:externalContent <http://mmisw.org/ont/mmi/device.ttl> ;
	     ] ;
	     ja:reasoner [
		ja:reasonerURL <http://jena.hpl.hp.com/2003/RDFSExptRuleReasoner> ;
	     ] ;
	     .
